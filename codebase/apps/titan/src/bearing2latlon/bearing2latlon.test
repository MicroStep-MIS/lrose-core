/****************************************************
 * TDRP params for bearing2latlon
 ****************************************************/

/*
 * Debug option.
 * If set, debug messages will be printed appropriately.
 *
 * Type: enum
 * Default: DEBUG_OFF 
 * Legal values: DEBUG_OFF, DEBUG_WARNINGS, DEBUG_NORM,
 *   DEBUG_EXTRA.
 */
debug = DEBUG_OFF;

/*
 * Malloc debug level.
 * 0 - none, 1 - corruption checking, 2 - records all malloc blocks
 *   and checks, 3 - printout of all mallocs etc.
 *
 * Type: long
 * Default: 0 
 * Min value: 0 
 * Max value: 3 
 */
malloc_debug_level = 0;

/*
 * Origin.
 * The origin about which the bearings were taken.
 *
 * Type: struct
 */
origin = {
  0, /* latitude:double */
  0  /* longitude:double */
};

/*
 * Input file.
 *
 * Type: string
 * Default: "none" 
 */
input_file = "none";


