/****************************************************
 * TDRP params for dva_mklookup
 ****************************************************/

/*
 * Debug option.
 * If set, debug messages will be printed appropriately.
 *
 * Type: enum
 * Default: DEBUG_OFF 
 * Legal values: DEBUG_OFF, DEBUG_WARNINGS, DEBUG_NORM,
 *   DEBUG_VERBOSE.
 */
debug = DEBUG_NORM;

/*
 * Malloc debug level.
 * 0 - none, 1 - corruption checking, 2 - records all malloc blocks
 *   and checks, 3 - printout of all mallocs etc.
 *
 * Type: long
 * Default: 0 
 * Min value: 0 
 * Max value: 3 
 */
malloc_debug_level = 0;

/*
 * Process instance.
 * Used for registration with procmap.
 *
 * Type: string
 * Default: "Test" 
 */
instance = "Test";

/*
 * Input file for RDAS calibration data.
 * This file is either produced manually, or written by dva_ingest.
 *
 * Type: string
 * Default: "rdas_cal_data" 
 */
rdas_cal_data_path = "$(DVA_HOME)/params/rdas_cal_data";

/*
 * File path for displace table.
 * This file is generated by this program.
 *
 * Type: string
 * Default: "displace_table" 
 */
displace_table_file_path = "$(DVA_HOME)/lookups/displace_table";

/*
 * File path for vip2byte table.
 * This file is generated by this program.
 *
 * Type: string
 * Default: "vip2byte" 
 */
vip2byte_table_file_path = "$(DVA_HOME)/lookups/vip2byte";

/*
 * File path for range_correction table.
 * This file is generated by this program.
 *
 * Type: string
 * Default: "range_correction" 
 */
range_correction_table_file_path = "$(DVA_HOME)/lookups/range_correction";

/*
 * Input directory for grid files.
 * These files are pre-computed by dva_mkgrid.
 *
 * Type: string
 * Default: "grids" 
 */
grid_files_dir = "$(DVA_HOME)/grids";

/*
 * Path for grid params ASCII file.
 * This file is created by dva_mkgrid.
 *
 * Type: string
 * Default: "grid_params" 
 */
grid_params_file_path = "$(DVA_HOME)/params/grid_params";

/*
 * Output directory for lookup files.
 * These files are used by dva_cart.
 *
 * Type: string
 * Default: "lookups" 
 */
lookup_files_dir = "$(DVA_HOME)/lookups";

/*
 * Array of radar elevation angles.
 * These are used by this program and incorporated into the lookup
 *   tables.
 *
 * Type: double
 * Array elem size: 8 bytes
 * Array has no max number of elements
 */
radar_elevations = {1.5, 3.0, 4.5, 6.0, 7.5, 9.0,
		    10.8, 13.0, 15.5, 18.4, 21.7,
		    25.6, 29.9, 34.6, 39.6, 44.8,
		    50.0, 55.0};
/*
 * Output file for radar elevations.
 * This file is used later by dva_cart.
 *
 * Type: string
 * Default: "radar_elevations" 
 */
radar_elevations_file_path = "radar_elevations";

