#
# debugging
#

cart_slave.debug: false
cart_slave.malloc_debug_level: 0

#
# communication
#

cart_slave.shmem_key: 43000
cart_slave.max_packet_length: 20000

#
# server mapper registration
#

cart_slave.cart_data_server_default_host: local
cart_slave.cart_data_server_default_port: 41000
cart_slave.cart_data_server_instance: Test
cart_slave.servmap_host1: local
cart_slave.servmap_host2: none

#
# slave table path
#

cart_slave.slave_table: $(TITAN_HOME)/tables/slave_table.test
